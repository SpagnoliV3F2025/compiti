#include <stdio.h>
#include <math.h>

int main() {
    double a, b, c, delta, x1, x2, x;
    scanf("%lf %lf %lf", &a, &b, &c);
    if (a == 0) {
        printf("l'equazione non e di secondo grado\n");
        printf("l'equazione non ha radici reali\n");
    } else {
        delta = b * b - 4 * a * c;
        if (delta > 0) {
            x1 = (-b + sqrt(delta)) / (2 * a);
            x2 = (-b - sqrt(delta)) / (2 * a);
            printf("%lf\n%lf\n", x1, x2);
        } else if (delta == 0) {
            x = -b / (2 * a);
            printf("%lf\n", x);
        } else {
            printf("l'equazione non ha radici reali\n");
        }
    }
    return 0;
}
